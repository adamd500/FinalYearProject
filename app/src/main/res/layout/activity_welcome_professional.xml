<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfessionalFeatures.WelcomeProfessional">


    <Button
        android:id="@+id/buttonBrowseJobs"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginLeft="52dp"
        android:onClick="getJobs"
        android:text="Browse for new job"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <Button
        android:id="@+id/buttonBrowseJobsOnMap"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginLeft="60dp"
        android:onClick="findJobOnMap"
        android:text="Browse New Jobs on Map"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.123" />

    <Button
        android:id="@+id/buttonAllJobs"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginLeft="60dp"
        android:onClick="currentJobs"
        android:text="Current Jobs"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.22" />

    <Button
        android:id="@+id/buttonEarnings"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginLeft="60dp"
        android:onClick="inbox"
        android:text="Inbox"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.323" />


    <Button
        android:id="@+id/buttonStripe"
        android:layout_width="356dp"
        android:layout_height="41dp"
        android:layout_marginLeft="24dp"
        android:onClick="setUpStripe"
        android:text="Set up payment service"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.76" />

    <Button
        android:id="@+id/buttonSchedule"
        android:layout_width="356dp"
        android:layout_height="41dp"
        android:layout_marginLeft="24dp"
        android:onClick="openCalendar"
        android:text="Calendar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.807" />

    <Button
        android:id="@+id/buttonGraph"
        android:layout_width="356dp"
        android:layout_height="41dp"
        android:layout_marginLeft="24dp"
        android:onClick="feedbackGraph"
        android:text="Feedback graph"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.701" />

    <Button
        android:id="@+id/buttonAcceptedConsultation"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginLeft="60dp"
        android:onClick="acceptedConsultations"
        android:text="Accepted Consultations"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.431" />

    <Button
        android:id="@+id/buttonFeedback"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginLeft="60dp"
        android:onClick="feedback"
        android:text="Feedback and Ratings"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.647" />

    <Button
        android:id="@+id/buttonCovid"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginLeft="60dp"
        android:onClick="covidInformation"
        android:text="Info on beign self employed durign covid"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.544" />

    <Button
        android:id="@+id/buttonProfile"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginLeft="60dp"
        android:onClick="viewProfile"
        android:text="View Profile"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.878" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:itemBackground="@color/cardview_shadow_start_color"
        app:itemIconTint="@color/white"
        app:itemTextColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:menu="@menu/menu_navigation"
        tools:layout_editor_absoluteX="208dp" />


</androidx.constraintlayout.widget.ConstraintLayout>