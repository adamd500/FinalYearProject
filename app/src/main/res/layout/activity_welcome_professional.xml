<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfessionalFeatures.WelcomeProfessional">

    <Button
        android:id="@+id/buttonBrowseJobs"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginLeft="52dp"
        android:text="Browse for new job"
        android:onClick="getJobs"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.099" />

    <Button
        android:id="@+id/buttonBrowseJobsOnMap"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginLeft="60dp"
        android:onClick="findJobOnMap"
        android:text="Browse New Jobs on Map"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.204" />

    <Button
        android:id="@+id/buttonAllJobs"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginLeft="60dp"
        android:onClick="currentJobs"
        android:text="Current Jobs"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.305" />

    <Button
        android:id="@+id/buttonEarnings"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginLeft="60dp"
        android:text="Inbox"
        android:onClick="inbox"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.414" />

    <Button
        android:id="@+id/buttonAcceptedConsultation"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginLeft="60dp"
        android:onClick="acceptedConsultations"
        android:text="Accepted Consultations"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.528" />

    <Button
        android:id="@+id/buttonFeedback"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginLeft="60dp"
        android:onClick="feedback"
        android:text="Feedback and Ratings"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.647" />

    <Button
        android:id="@+id/buttonProfile"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginLeft="60dp"
        android:text="View Profile"
        android:onClick="viewProfile"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.936" />
</androidx.constraintlayout.widget.ConstraintLayout>