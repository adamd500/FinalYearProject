<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfessionalFeatures.ProfessionalProfile">

    <TextView
        android:id="@+id/textViewDocumentText"
        android:layout_width="334dp"
        android:layout_height="36dp"
        android:layout_marginLeft="12dp"
        android:text="Any outstanding documents will apear below :"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.525" />

    <TextView
        android:id="@+id/textViewIdRequired"
        android:layout_width="368dp"
        android:layout_height="70dp"
        android:layout_marginLeft="12dp"
        android:text="Required Documents : Identification, Garda Vet and Safe Pass\n Please Upload an Image of each document to verify your identity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.623" />
    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:itemBackground="@color/colorPrimary"
        app:itemIconTint="@color/white"
        app:itemTextColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:menu="@menu/menu_navigation"
        tools:layout_editor_absoluteX="208dp" />
    <Button
        android:id="@+id/buttonID"
        android:layout_width="363dp"
        android:layout_height="40dp"
        android:layout_marginLeft="24dp"
        android:onClick="selectId"
        android:text="Upload ID Document"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.733" />

    <Button
        android:id="@+id/buttonSelfie"
        android:layout_width="363dp"
        android:layout_height="40dp"
        android:layout_marginLeft="24dp"
        android:onClick="selectSelfie"
        android:text="Upload Selfie Image"
        app:layout_constraintBottom_toBottomOf="parent"
